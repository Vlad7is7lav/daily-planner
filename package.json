    "prepare": "husky install",
    "pretty": "prettier --write src/**/*.{js,jsx,ts,tsx,md,html,css} server/**/*.{js,jsx,ts,tsx,md,html,css}"
  },
  "lint-staged": {
    "**/*": [
      "npm run pretty",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:3003",
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
